{
  "extends": "semistandard",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "_t": true,
    "__ENV__": true,
    "grunt": true
  },
  "rules": {
    "camelcase": "off",
    "indent": "off",
    "no-mixed-operators": "off",
    "no-useless-escape": "off",
    "no-useless-computed-key": "off",
    "no-multiple-empty-lines": "off",
    "no-useless-return": "off"
  }
}
