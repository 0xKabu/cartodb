{
  "extends": "semistandard",
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "$": true,
    "cdb": true,
    "Clipboard": true
  },
  "rules": {
    "space-before-keywords": "off",
    "space-after-keywords": "off",
    "space-return-throw-case": "off",
    "no-mixed-operators": "off",
    "no-use-before-define": "off",
    "no-multiple-empty-lines": "off"
  }
}
